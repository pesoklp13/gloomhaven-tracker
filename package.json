    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "story:party": "nx run gloomhaven-party-components:storybook",
    "commit-msg": "tsc --p tsconfig.scripts.json && node ./dist/scripts/commitLint.js"
